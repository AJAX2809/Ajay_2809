        "express": "^4.21.2",
        "express-session": "^1.18.1",
        "memorystore": "^1.6.7",
        "passport": "^0.7.0",
        "passport-local": "^1.0.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.55.0",
        "recharts": "^2.15.2",
        "wouter": "^3.3.5",
